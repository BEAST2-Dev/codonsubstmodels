    <map name="Uniform" >beast.base.inference.distribution.Uniform</map>
    <map name="Exponential" >beast.base.inference.distribution.Exponential</map>
    <map name="LogNormal" >beast.base.inference.distribution.LogNormalDistributionModel</map>
    <map name="Normal" >beast.base.inference.distribution.Normal</map>
    <map name="Beta" >beast.base.inference.distribution.Beta</map>
    <map name="Gamma" >beast.base.inference.distribution.Gamma</map>
    <map name="LaplaceDistribution" >beast.base.inference.distribution.LaplaceDistribution</map>
    <map name="prior" >beast.base.inference.distribution.Prior</map>
    <map name="InverseGamma" >beast.base.inference.distribution.InverseGamma</map>
    <map name="OneOnX" >beast.base.inference.distribution.OneOnX</map>

    <run id="mcmc" spec="MCMC" chainLength="2000000">
        <state id="state" spec="State" storeEvery="5000">
            <tree id="Tree.t:aacodon" spec="beast.base.evolution.tree.Tree" name="stateNode">
                <taxonset id="TaxonSet.aacodon" spec="TaxonSet">
                    <alignment id="aacodon" spec="codonmodels.evolution.alignment.CodonAlignmentFromAminoAcids" geneticCode="vertebrateMitochondrial"
                    	dataType="codon">
                        <data idref="codon"/>
                    </alignment>
                </taxonset>
            </tree>
            <parameter id="M0.omega.s:aacodon" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
            <parameter id="gammaShape.s:aacodon" spec="parameter.RealParameter" lower="0.1" name="stateNode">1.0</parameter>
            <parameter id="popSize.t:aacodon" spec="parameter.RealParameter" lower="0.0" name="stateNode">0.3</parameter>
	        <stateNode id="BMT_ModelIndicator.s:aacodon" spec="parameter.IntegerParameter" lower="0" upper="5">5</stateNode>
    	    <parameter id="BMT_Rates.s:aacodon" dimension="6" lower="0.01" name="stateNode" upper="100.0">1.0</parameter>

	        <stateNode id="BMT_ModelIndicator2.s:aacodon" spec="parameter.IntegerParameter" lower="0" upper="5">5</stateNode>
    	    <parameter id="BMT_Rates2.s:aacodon" dimension="6" lower="0.01" name="stateNode" upper="100.0">1.0</parameter>
	        <stateNode id="BMT_ModelIndicator3.s:aacodon" spec="parameter.IntegerParameter" lower="0" upper="5">5</stateNode>
    	    <parameter id="BMT_Rates3.s:aacodon" dimension="6" lower="0.01" name="stateNode" upper="100.0">1.0</parameter>

            <stateNode idref="BMT_frequencies1.s:aacodon"/>
            <!-->
            <stateNode idref="BMT_frequencies2.s:aacodon"/>
            <stateNode idref="BMT_frequencies3.s:aacodon"/>
            -->
            <parameter id="mnmPenalty.s:aacodon" dimension="2" spec="parameter.RealParameter" name="stateNode">0.0</parameter>
            <parameter id="substitutionRate.s:aacodon" dimension="3" lower="0" upper="3" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
        </state>
        
        <init id="RandomTree.t:aacodon" spec="RandomTree" estimate="false" initial="@Tree.t:aacodon" taxa="@aacodon">
            <populationModel id="ConstantPopulation0.t:aacodon" spec="ConstantPopulation">
                <parameter id="randomPopSize.t:aacodon" spec="parameter.RealParameter" name="popSize">1.0</parameter>
            </populationModel>
        </init>
        
        <distribution id="posterior" spec="CompoundDistribution">
            <distribution id="prior" spec="CompoundDistribution">
                <distribution id="CoalescentConstant.t:aacodon" spec="Coalescent">
                    <populationModel id="ConstantPopulation.t:aacodon" spec="ConstantPopulation" popSize="@popSize.t:aacodon"/>
                    <treeIntervals id="TreeIntervals.t:aacodon" spec="beast.base.evolution.tree.TreeIntervals" tree="@Tree.t:aacodon"/>
                </distribution>
                
		        <distribution id="BMT_RatesPrior.s:aacodon" spec="bmodeltest.math.distributions.NucleotideRevJumpSubstModelRatePrior" modelIndicator="@BMT_ModelIndicator.s:aacodon" x="@BMT_Rates.s:aacodon"
        		    priorType="onTransitionsAndTraversals" substModel="@RevJump.s:aacodon"/>
		        <distribution id="BMT_RatesPrior2.s:aacodon" spec="bmodeltest.math.distributions.NucleotideRevJumpSubstModelRatePrior" modelIndicator="@BMT_ModelIndicator2.s:aacodon" x="@BMT_Rates2.s:aacodon"
        		    priorType="onTransitionsAndTraversals" substModel="@RevJump2.s:aacodon"/>
		        <distribution id="BMT_RatesPrior3.s:aacodon" spec="bmodeltest.math.distributions.NucleotideRevJumpSubstModelRatePrior" modelIndicator="@BMT_ModelIndicator3.s:aacodon" x="@BMT_Rates3.s:aacodon"
        		    priorType="onTransitionsAndTraversals" substModel="@RevJump3.s:aacodon"/>

                <prior id="OmegaPrior.s:aacodon" name="distribution" x="@M0.omega.s:aacodon">
                    <LogNormal id="LogNormalDistributionModel.0" name="distr">
                        <parameter id="RealParameter.0" spec="parameter.RealParameter" estimate="false" name="M">-3.0</parameter>
                        <parameter id="RealParameter.1" spec="parameter.RealParameter" estimate="false" name="S">1.25</parameter>
                    </LogNormal>
                </prior>
                <prior id="GammaShapePrior.s:aacodon" name="distribution" x="@gammaShape.s:aacodon">
                    <Exponential id="Exponential.0" name="distr">
                        <parameter id="RealParameter.4" spec="parameter.RealParameter" estimate="false" name="mean">1.0</parameter>
                    </Exponential>
                </prior>
                <prior id="PopSizePrior.t:aacodon" name="distribution" x="@popSize.t:aacodon">
                    <OneOnX id="OneOnX.1" name="distr"/>
                </prior>
                <!-->
                <prior id="mnmPenaltyePrior.s:aacodon" name="distribution" x="@mnmPenalty.s:aacodon">
                    <LogNormal name="distr" M="-3.0" S="1.25"/>
                </prior>
                -->
            </distribution>
            <distribution id="likelihood" spec="CompoundDistribution" useThreads="true">
                <distribution id="treeLikelihood.aacodon" spec="ThreadedTreeLikelihood" data="@aacodon" tree="@Tree.t:aacodon" useAmbiguities="true">
                    <siteModel id="SiteModel.s:aacodon" spec="SiteModel" gammaCategoryCount="4" shape="@gammaShape.s:aacodon">
                        <parameter id="mutationRate.s:aacodon" spec="parameter.RealParameter" estimate="false" name="mutationRate">1.0</parameter>
                        <parameter id="proportionInvariant.s:aacodon" spec="parameter.RealParameter" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                        
                        <substModel id="GCS.s:aacodon" spec="codonmodels.GeneralCodonSubstitutionModel" omega="@M0.omega.s:aacodon"
        	                substitutionRate="@substitutionRate.s:aacodon"
                        	mnmPenalty="@mnmPenalty.s:aacodon">
                        	<substModel id="RevJump.s:aacodon" spec="bmodeltest.evolution.substitutionmodel.NucleotideRevJumpSubstModel" 
        	                modelIndicator="@BMT_ModelIndicator.s:aacodon" 
        	                rates="@BMT_Rates.s:aacodon"
            	            modelSet="transitionTransversionSplit">
                	    	    <frequencies id="BMTfreqs.s:aacodon" spec="Frequencies" frequencies="0.25 0.25 0.25 0.25"/>
		            	    </substModel>
                        	<substModel id="RevJump2.s:aacodon" spec="bmodeltest.evolution.substitutionmodel.NucleotideRevJumpSubstModel" 
        	                modelIndicator="@BMT_ModelIndicator2.s:aacodon" 
        	                rates="@BMT_Rates2.s:aacodon"
            	            modelSet="transitionTransversionSplit">
                	    	    <frequencies id="BMTfreqs2.s:aacodon" spec="Frequencies" frequencies="0.25 0.25 0.25 0.25"/>
		            	    </substModel>
                        	<substModel id="RevJump3.s:aacodon" spec="bmodeltest.evolution.substitutionmodel.NucleotideRevJumpSubstModel" 
        	                modelIndicator="@BMT_ModelIndicator3.s:aacodon" 
        	                rates="@BMT_Rates3.s:aacodon"
            	            modelSet="transitionTransversionSplit">
                	    	    <frequencies id="BMTfreqs3.s:aacodon" spec="Frequencies" frequencies="0.25 0.25 0.25 0.25"/>
		            	    </substModel>
		            	    
		            	    
                            <frequencies id="codonFreqs.s:aacodon" spec="codonmodels.GeneralCodonFrequencies" estimate="false"> 
<!--
                            	<frequencies id="BMT_frequencies.s:aacodon" spec="beast.base.inference.parameter.RealParameter" lower="0.0" upper="1.0" dimension="60" value="0.0166666666666667"/>
-->
                            	<freq id="BMT_frequencies1.s:aacodon" spec="beast.base.inference.parameter.RealParameter" lower="0.0" upper="1.0" dimension="4" value="0.25"/>
                            	<!-->
                            	<freq id="BMT_frequencies2.s:aacodon" spec="beast.base.inference.parameter.RealParameter" lower="0.0" upper="1.0" dimension="4" value="0.25"/>
                            	<freq id="BMT_frequencies3.s:aacodon" spec="beast.base.inference.parameter.RealParameter" lower="0.0" upper="1.0" dimension="4" value="0.25"/>
                            	-->
                            </frequencies>
                        </substModel>
                    </siteModel>
                    <branchRateModel id="StrictClock.c:aacodon" spec="beast.base.evolution.branchratemodel.StrictClockModel">
                        <parameter id="clockRate.c:aacodon" spec="parameter.RealParameter" estimate="false" name="clock.rate">1.0</parameter>
                    </branchRateModel>
                </distribution>
            </distribution>
        </distribution>
        
        <operator id="OmegaScaler.s:aacodon" spec="ScaleOperator" parameter="@M0.omega.s:aacodon" scaleFactor="0.5" weight="1.0"/>
        <operator id="gammaShapeScaler.s:aacodon" spec="ScaleOperator" parameter="@gammaShape.s:aacodon" scaleFactor="0.5" weight="0.1"/>
        <operator id="CoalescentConstantBICEPSEpochTop.t:aacodon" spec="EpochFlexOperator" scaleFactor="0.1" tree="@Tree.t:aacodon" weight="2.0"/>
        <operator id="CoalescentConstantBICEPSEpochAll.t:aacodon" spec="EpochFlexOperator" fromOldestTipOnly="false" scaleFactor="0.1" tree="@Tree.t:aacodon" weight="2.0"/>
        <operator id="CoalescentConstantBICEPSTreeFlex.t:aacodon" spec="TreeStretchOperator" scaleFactor="0.01" tree="@Tree.t:aacodon" weight="2.0"/>
        <operator id="CoalescentConstantTreeRootScaler.t:aacodon" spec="kernel.BactrianScaleOperator" rootOnly="true" scaleFactor="0.1" tree="@Tree.t:aacodon" upper="10.0" weight="3.0"/>
        <operator id="CoalescentConstantUniformOperator.t:aacodon" spec="kernel.BactrianNodeOperator" tree="@Tree.t:aacodon" weight="30.0"/>
        <operator id="CoalescentConstantSubtreeSlide.t:aacodon" spec="kernel.BactrianSubtreeSlide" tree="@Tree.t:aacodon" weight="15.0"/>
        <operator id="CoalescentConstantNarrow.t:aacodon" spec="Exchange" tree="@Tree.t:aacodon" weight="15.0"/>
        <operator id="CoalescentConstantWide.t:aacodon" spec="Exchange" isNarrow="false" tree="@Tree.t:aacodon" weight="3.0"/>
        <operator id="CoalescentConstantWilsonBalding.t:aacodon" spec="WilsonBalding" tree="@Tree.t:aacodon" weight="3.0"/>
        <operator id="PopSizeScaler.t:aacodon" spec="kernel.BactrianScaleOperator" parameter="@popSize.t:aacodon" upper="10.0" weight="3.0"/>
        
        <operator id="BMT_ModelTestOperator.s:aacodon" spec="bmodeltest.evolution.operators.BMTMergeSplitOperator" modelIndicator="@BMT_ModelIndicator.s:aacodon" rates="@BMT_Rates.s:aacodon" substModel="@RevJump.s:aacodon" weight="1.0"/>
        <operator id="BMT_Ratescaler.s:aacodon" spec="bmodeltest.evolution.operators.BMTExchangeOperator" modelIndicator="@BMT_ModelIndicator.s:aacodon" rates="@BMT_Rates.s:aacodon" substModel="@RevJump.s:aacodon" delta="0.15" weight="1.0"/>

        <operator id="BMT_ModelTestOperator2.s:aacodon" spec="bmodeltest.evolution.operators.BMTMergeSplitOperator" modelIndicator="@BMT_ModelIndicator2.s:aacodon" rates="@BMT_Rates2.s:aacodon" substModel="@RevJump2.s:aacodon" weight="1.0"/>
        <operator id="BMT_Ratescaler2.s:aacodon" spec="bmodeltest.evolution.operators.BMTExchangeOperator" modelIndicator="@BMT_ModelIndicator2.s:aacodon" rates="@BMT_Rates2.s:aacodon" substModel="@RevJump2.s:aacodon" delta="0.15" weight="1.0"/>

        <operator id="BMT_ModelTestOperator3.s:aacodon" spec="bmodeltest.evolution.operators.BMTMergeSplitOperator" modelIndicator="@BMT_ModelIndicator3.s:aacodon" rates="@BMT_Rates3.s:aacodon" substModel="@RevJump3.s:aacodon" weight="1.0"/>
        <operator id="BMT_Ratescaler3.s:aacodon" spec="bmodeltest.evolution.operators.BMTExchangeOperator" modelIndicator="@BMT_ModelIndicator3.s:aacodon" rates="@BMT_Rates3.s:aacodon" substModel="@RevJump3.s:aacodon" delta="0.15" weight="1.0"/>

        <operator id='BMT_FrequenciesExchanger1.s:aacodon' spec='kernel.BactrianDeltaExchangeOperator' delta="0.01" weight="0.2" parameter="@BMT_frequencies1.s:aacodon"/>
        <!-->
        <operator id='BMT_FrequenciesExchanger2.s:aacodon' spec='kernel.BactrianDeltaExchangeOperator' delta="0.01" weight="0.2" parameter="@BMT_frequencies2.s:aacodon"/>
        <operator id='BMT_FrequenciesExchanger3.s:aacodon' spec='kernel.BactrianDeltaExchangeOperator' delta="0.01" weight="0.2" parameter="@BMT_frequencies3.s:aacodon"/>
        -->
        <operator id="BMT_mnmPenaltyScaler.s:aacodon" spec="ScaleOperator" parameter="@mnmPenalty.s:aacodon" scaleFactor="0.5" weight="1.0"/>
        <!-->
        <operator id='BMT_SubstRateExchanger.s:aacodon' spec='kernel.BactrianDeltaExchangeOperator' delta="0.01" weight="0.2" parameter="@substitutionRate.s:aacodon"/>
        -->

        <operator id="updown.s:aacodon" spec="UpDownOperator" scaleFactor="0.5" weight="5.0">
        	<up idref="Tree.t:aacodon"/>
        	<down idref="M0.omega.s:aacodon"/>
        </operator>

        <logger id="tracelog" spec="Logger" fileName="$(filebase).log" logEvery="1000" model="@posterior" sanitiseHeaders="true" sort="smart">
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
            <log id="TreeHeight.t:aacodon" spec="beast.base.evolution.tree.TreeStatLogger" tree="@Tree.t:aacodon"/>
            <log idref="M0.omega.s:aacodon"/>
            <log idref="gammaShape.s:aacodon"/>
            <log idref="popSize.t:aacodon"/>
            <log idref="CoalescentConstant.t:aacodon"/>
            <log idref="RevJump.s:aacodon"/>
            <log idref="BMT_ModelIndicator.s:aacodon"/>
            <log idref="BMT_frequencies1.s:aacodon"/>

            <log idref="RevJump2.s:aacodon"/>
            <log idref="BMT_ModelIndicator2.s:aacodon"/>

            <log idref="RevJump3.s:aacodon"/>
            <log idref="BMT_ModelIndicator3.s:aacodon"/>
            <!-->
            <log idref="BMT_frequencies2.s:aacodon"/>
            <log idref="BMT_frequencies3.s:aacodon"/>
            -->
            <log idref="mnmPenalty.s:aacodon"/>
            <log idref="substitutionRate.s:aacodon"/>            
        </logger>
        
        <logger id="screenlog" spec="Logger" logEvery="1000">
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
            <log idref="M0.omega.s:aacodon"/>
        </logger>
        
        <logger id="treelog.t:aacodon" spec="Logger" fileName="$(filebase).trees" logEvery="1000" mode="tree">
            <log id="TreeWithMetaDataLogger.t:aacodon" spec="beast.base.evolution.TreeWithMetaDataLogger" tree="@Tree.t:aacodon"/>
        </logger>
    </run>

</beast>
