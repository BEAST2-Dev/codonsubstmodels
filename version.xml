        <provider classname="codonmodels.app.beauti.CodonAlignmentProvider"/>
        <provider classname="codonmodels.ApproximateSubstModel"/>
        <provider classname="codonmodels.ApproxP_dist_CubicSpline"/>
        <provider classname="codonmodels.ApproxP_dist_Piecewise"/>
        <provider classname="codonmodels.ApproxP_dist_Polynomial"/>
        <provider classname="codonmodels.CodonFrequencies"/>
        <provider classname="codonmodels.CodonSubstitutionModel"/>
        <provider classname="codonmodels.evolution.alignment.CodonAlignment"/>
        <provider classname="codonmodels.evolution.datatype.Codon"/>
        <provider classname="codonmodels.evolution.likelihood.DataAugTreeLikelihood"/>
