        <provider classname="codonmodels.ApproxP_dist_Polynomial"/>
        <provider classname="codonmodels.CodonFrequencies"/>
        <provider classname="codonmodels.CodonSubstitutionModel"/>
        <provider classname="codonmodels.M0Model"/>
