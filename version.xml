    <service type="beast.base.evolution.datatype.DataType">
        <provider classname="codonmodels.evolution.datatype.Codon"/>
    </service>

    <service type="beastfx.app.inputeditor.InputEditor">
        <provider classname="codonmodels.app.beauti.CodonFrequenciesInputEditor"/>
    </service>


</package>

